<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>mypkg.Contextlistener</listener-class>
    </listener>
    <listener>
        <description>HttpSessionListener</description>
        <listener-class>mypkg.sessionhandle</listener-class>
    </listener>
    <listener>
        <description>RequestListener</description>
        <listener-class>requesthandle</listener-class>
    </listener>
    <servlet>
        <servlet-name>Timetable</servlet-name>
        <servlet-class>Timetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Syllabus</servlet-name>
        <servlet-class>Syllabus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Labscheduals</servlet-name>
        <servlet-class>Labscheduals</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Endsession</servlet-name>
        <servlet-class>Endsession</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Timetable</servlet-name>
        <url-pattern>/Timetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Syllabus</servlet-name>
        <url-pattern>/Syllabus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Labscheduals</servlet-name>
        <url-pattern>/Labscheduals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Endsession</servlet-name>
        <url-pattern>/Endsession</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
